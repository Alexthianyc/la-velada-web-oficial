{
  "headers": [
    {
      "source": "/(.*).(avif|webp|svg|png|jpg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, stale-while-revalidate=120"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "/2024/:path*",
      "permanent": true
    }
  ]
}
